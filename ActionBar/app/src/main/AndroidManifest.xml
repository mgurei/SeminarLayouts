<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.personalportablehealthtechnology.actionbar">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        android:id="@+id/my_action_bar"
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity
            android:name="com.personalportablehealthtechnology.actionbar.second_activity"
            android:label="@string/title_second_activity"
            android:parentActivityName="com.personalportablehealthtechnology.actionbar.MainActivity" >

        </activity>


    </application>

</manifest>